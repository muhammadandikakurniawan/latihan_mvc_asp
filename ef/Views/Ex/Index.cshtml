@section content{
    @if (TempData.Peek("message") != null && TempData.Peek("type") != null)
    {
        <div class="alert alert-@TempData["type"] alert-dismissible fade show" role="alert">
            @TempData["message"]
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    }
    <div class="jumbotron">
        <h1 class="display-4">This Is Web -_-</h1>
        <p class="lead">This is a simple wbsite</p>

    </div>

}